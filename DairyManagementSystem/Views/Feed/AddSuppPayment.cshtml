@model List<DairyManagementSystem.Models.SupplierPayModel>
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>AddSuppPayment</title>
</head>
<style>
    body {
        background-image: url('../../Content/Payment3 Img.jpg');
        background-size: cover;
    }
</style>
<body>
    <h1>Feed Supplier</h1>
    <ul>
        <li><a href="~/Feed/FeedIndex">Home</a></li>
        <li><a href="~/Feed/AddFeedItem">Add New Item</a></li>
        <li><a href="~/Feed/AddFeedTrans">Sell Feed</a></li>
        <li><a href="~/Feed/ViewFeedTransold">View Sales List</a></li>
        <li><a class="active" href="~/Feed/AddSuppPayment">Generate Vendor Payment for Feed</a></li>
        <li style="float:right"><a href="~/Home/Index">Log out</a></li>
    </ul>
    <p></p>
    <a href="~/Feed/FeedIndex"><input type="image" src="~/Content/back.png" width="48" height="48" /></a>
    <p></p>
    <div id="example2">
        <form method="post">
            
                <h1 style=" text-align-last:center"> Add Vendor Payment for Feed </h1><br />
                <table border:"2" align="center" cellspacing="10px">
                    <tr>
                        <td>
                            <h5>Month</h5>
                        </td>
                        <td>&nbsp;</td>
                        <td>&nbsp;</td>
                        <td>
                            <h5>Year</h5>
                        </td>
                        <td>&nbsp;</td>
                        <td>&nbsp;</td>
                        <td>
                            <h5>Vendor Id</h5>
                        </td>
                        @*<td>&nbsp;</td>
                            <td>&nbsp;</td>
                            <td>
                                <h5>Date</h5>
                            </td>*@
                    </tr>
                    <tr>
                        <td>
                            @Html.DropDownList("month")
                        </td>
                        <td>&nbsp;</td>
                        <td>&nbsp;</td>
                        <td>
                            @Html.DropDownList("year")
                        </td>
                        <td>&nbsp;</td>
                        <td>&nbsp;</td>
                        <td>
                            @Html.DropDownList("UserId", (List<SelectListItem>)ViewBag.vend)
                        </td>
                        <tr>
                            <td>
                                <input type="submit" value="SUBMIT" class="btn btn-default" />
                            </td>
                        @*<td>&nbsp;</td>
                            <td>&nbsp;</td>
                            <td>
                                @Html.TextBox("date", new { type = "date" })
                            </td>*@
                    </tr>
                </table>
                
            </div>
        </form>
        @ViewBag.result
</body>
</html>
<link href="~/Content/StyleSheet1.css" rel="stylesheet" />
