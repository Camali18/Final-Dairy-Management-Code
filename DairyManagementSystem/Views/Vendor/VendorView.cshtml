@model List<DairyManagementSystem.Models.HistoryModel>

@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>VendorView</title>
</head>
<style>
    body {
        background-image: url('../../Content/Payment3 Img.jpg');
        background-size: cover;
    }
</style>
<body>
    <h1>Vendor</h1>
    <ul>
        <li><a class="active" href="~/Vendor/VendorView">Home</a></li>
        <li><a href="~/Vendor/ViewFeedItem">View FeedItem List</a></li>
        <li><a href="~/Vendor/ViewPaymentSlip">View PaymentSlip</a></li>
        <li style="float:right"><a href="~/Home/Index">Log out</a></li>
    </ul>
    <p></p>
    <a href="~/Vendor/VendorView"><input type="image" src="~/Content/back.png" width="48" height="48" /></a>
    <p></p>
    <div id="example2">
        <form method="post">
           
            <table  align="center" cellspacing="30px">
                <caption style="font-size:x-large">Daily Milk Data..!</caption>
                <tr>
                    <td>
                        <b>Vendor Name</b>
                    </td>
                    <td>
                        <b>Milk Quantity</b>
                    </td>
                    <td>
                        <b>FatPercent</b>
                    </td>
                    <td>
                        <b>Time</b>
                    </td>
                    <td>
                        <b>Date</b>
                    </td>
                    <td>
                        <b>Price</b>
                    </td>

                </tr>
                @foreach (var i in Model)
                {
                    <tr>
                        <td>
                            @i.VendorName
                        </td>
                        <td>
                            @i.quantity
                        </td>
                        <td>
                            @i.fat
                        </td>
                        <td>
                            @i.timing
                        </td>
                        <td>
                            @i.date
                        </td>
                        <td>
                            @i.Price
                        </td>
                    </tr>
                }
            </table>
        </form>
        
</body>
</html>
<link href="~/Content/StyleSheet1.css" rel="stylesheet" />
